{
    "manifest_version": 2,
    "name": "C&R",
    "description": "does something stuipid",
    "version": "1",
    "author": "Chris and Ricardo",
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ],
            "matches": [
                "*://*.lateral.io/*"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "tab-icon.png",
        "default_title": "Have a good day",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "activeTab"
    ]
}